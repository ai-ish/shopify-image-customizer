{
  "sections": {
    "main": {
      "type": "product",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "subheading_option": "breadcrumb",
            "text_size": 6,
            "show_rating": true,
            "enable_star_rating": true,
            "show_rating_count": false,
            "padding_bottom": 8
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "padding_bottom": 14
          }
        },
        "image_customizer": {
          "type": "image-customizer",
          "settings": {
            "title": "Customize Your Image",
